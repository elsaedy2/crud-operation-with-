@model List<Room>



<link href="~/css/tabeldesign.css" rel="stylesheet" />
<link href="~/css/linkdesign.css" rel="stylesheet" />
<a class="nav-link text-dark" asp-area="" asp-controller="Room" asp-action="Create">Add new student</a>

<table>

    <tr>
        <th>
            Room Name
        </th>
       
        <th>
            Delete
        </th>
    </tr>

    @foreach (Room room in Model)
    {
        <tr>
            <td>
                @room.RoomName
            </td>
           
            <td>
                <a href="/Student/Delete/@room.RoomId">X</a>
            </td>
        </tr>
    }


</table>
